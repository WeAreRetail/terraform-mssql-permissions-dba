{
  "cSpell.customDictionaries": {
    "project-words": {
      "addWords": true,
      "description": "Words used in this project",
      "name": "project-words",
      "path": "${workspaceRoot}/.vscode/cspell-project-words.txt"
    }
  },
  "cSpell.dictionaries": ["sql"],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "files.associations": {
    "**/.azuredevops/**/*.yml": "azure-pipelines",
    "**/.azuredevops/**/variables/*.yml": "yaml",
    "*.tfbackend": "terraform"
  },
  "vsicons.associations.folders": [
    {
      "extensions": ["windows", "win32", "win"],
      "format": "svg",
      "icon": "windows"
    },
    {
      "extensions": ["azure", "azurerm"],
      "format": "svg",
      "icon": "azure"
    },
    {
      "extensions": [
        "azure-pipelines",
        ".azure-pipelines",
        ".azuredevops",
        "azuredevops"
      ],
      "format": "svg",
      "icon": "azurepipelines"
    }
  ],
  "workbench.iconTheme": "vscode-icons"
}
